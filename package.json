		"eslint-plugin-svelte": "^3.9.2",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.34.6",
		"svelte-check": "^4.2.1",
		"svelte-dnd-action": "^0.9.61",
