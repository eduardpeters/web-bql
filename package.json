		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^16.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-dnd-action": "^0.9.57",
		"typescript": "^5.7.3",
